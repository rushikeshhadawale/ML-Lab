   "source": [
    "data = {'Value': [10, 20, 100, 30, 40]}\n",
    "df = pd.DataFrame(data)\n",
    "z_scores = np.abs(stats.zscore(df))\n",
    "df_no_outliers = df[(z_scores < 3).all(axis=1)]\n",
    "print(df_no_outliers)"
   ]
